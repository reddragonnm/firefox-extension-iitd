{
  "manifest_version": 2,
  "name": "IITD login",
  "version": "1.0",
  "description": "Easy login to IITD moodle",
  "content_scripts": [
    {
      "matches": ["https://moodle.iitd.ac.in/login/index.php"],
      "js": ["main.js"]
    }
  ],
  "permissions": [
    "storage",
    "activeTab"
  ],
  "browser_action": {}
}
